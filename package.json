{
  "name": "guidemaker-default-template",
  "version": "5.1.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/empress/guidemaker-default-template",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "auto-changelog --template changelog.template --unreleased-only --prepend --load-github-issue-data --github-cache-dir .changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "ember-compatibility-tests": "ember try:each"
  },
  "dependencies": {
    "algoliasearch": "^4.0.0",
    "broccoli-funnel": "^3.0.2",
    "ember-auto-import": "^2.4.2",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-cli-sass": "^10.0.1",
    "ember-collapsible-panel": "^6.0.1",
    "ember-concurrency": "^2.0.2",
    "ember-href-to": "^5.0.0",
    "ember-power-select": "^6.0.0",
    "ember-root-url": "^1.0.1",
    "ember-showdown-prism": "^3.2.0",
    "ember-tether": "^2.0.0",
    "ember-truth-helpers": "^3.0.0",
    "sass": "^1.22.4"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "auto-changelog": "github:mansona/auto-changelog#epic",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.5",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~3.28.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "https://git@github.com/ember-cli/ember-try.git#master",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "guidemaker": "^3.1.0",
    "guidemaker-link-checker": "^1.0.0",
    "guidemaker-toc-checker": "^1.0.0",
    "lint-to-the-future": "^1.1.0",
    "lint-to-the-future-ember-template": "^1.1.0",
    "lint-to-the-future-eslint": "^1.0.0",
    "loader.js": "^4.7.0",
    "mocha": "^8.3.1",
    "npm-run-all": "^4.1.5",
    "prember": "^1.1.1",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
